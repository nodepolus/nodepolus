    "lint-and-fix": "eslint . --ext .ts --fix",
    "publish": "npm publish --registry https://npm.polus.gg"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NodePolus/nodepolus.git"
  },
