{
  "name": "@nodepolus/nodepolus",
  "version": "3.0.0",
  "description": "An open-source Among Us server implementation in TypeScript",
  "engines": {
    "node": ">=15"
  },
  "scripts": {
    "dev": "NP_LOG_LEVEL=trace npm start",
